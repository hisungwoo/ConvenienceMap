<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".fragments.InfoFragment">


    <ImageView
        android:id="@+id/info_img"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="55dp"
        android:src="@drawable/evalinfo_accessible_img"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />


    <TextView
        android:id="@+id/info_name_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:text="앱 개발자 : 131 dev"
        app:layout_constraintTop_toBottomOf="@+id/info_img"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <TextView
        android:id="@+id/info_mail_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이메일 : ilsamildev@gmail.com"
        app:layout_constraintTop_toBottomOf="@+id/info_name_tv"
        app:layout_constraintStart_toStartOf="@+id/info_name_tv"
        />

    <TextView
        android:id="@+id/info_version_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="버전 : 22.1.1"
        app:layout_constraintTop_toBottomOf="@+id/info_mail_tv"
        app:layout_constraintStart_toStartOf="@+id/info_mail_tv"
        />

    <TextView
        android:id="@+id/info_license_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="오픈소스 라이센스 : "
        app:layout_constraintTop_toBottomOf="@+id/info_version_tv"
        app:layout_constraintStart_toStartOf="@+id/info_version_tv"
        />

    <TextView
        android:id="@+id/info_license_kakao_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="· kakaoMap"
        app:layout_constraintTop_toBottomOf="@+id/info_license_tv"
        app:layout_constraintStart_toStartOf="@+id/info_license_tv"
        />

    <TextView
        android:id="@+id/info_license_google_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="· google streetView"
        app:layout_constraintTop_toBottomOf="@+id/info_license_kakao_tv"
        app:layout_constraintStart_toStartOf="@+id/info_license_kakao_tv"
        />

    <TextView
        android:id="@+id/info_license_open_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="· openApi"
        app:layout_constraintTop_toBottomOf="@+id/info_license_google_tv"
        app:layout_constraintStart_toStartOf="@+id/info_license_google_tv"
        />




</androidx.constraintlayout.widget.ConstraintLayout>